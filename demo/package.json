{
  "name": "lightweight-charts-demo",
  "version": "1.0.0",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "main": "main.ts",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "build-watch": "tsc -w",
    "start": "node ./dist/main.js",
    "start-watch": "node --watch-path=./src ./dist/main.js",
    "dev": "npm run build && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:build-watch\" \"npm:start-watch\"",
    "serve": "npx webpack serve --config webpack.config.js --progress --open"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.12.7",
    "concurrently": "^8.2.2",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "ts-loader": "9.4.2",
    "typescript": "4.9.5",
    "html-webpack-plugin": "5.5.3",
    "webpack": "5.81.0",
    "webpack-cli": "5.0.2",
    "webpack-dev-server": "4.15.1"
  }
}
